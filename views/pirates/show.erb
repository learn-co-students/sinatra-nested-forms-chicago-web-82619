<h1> Pirate </h1>
<div class="pirate">
  <h3>Name: <%= @pirate.name %></h3>

<h1>Weight: <%= @pirate.weight %></h1>
<h1>Height: <%= @pirate.height %></h1>
</div>

<h1>Ships</h1>
<% @ships.each do |ship| %>
  <div class="ship">
    <p>Name: <%= ship[:name] %></p>
    <p>type: <%= ship[:type] %></p>
    <p>booty: <%= ship[:booty] %></p>
  </div>
<% end %>
